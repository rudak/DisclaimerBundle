{
    "name": "rudak/disclaimer-bundle",
    "type": "symfony-bundle",
    "description": "Modern Symfony Bundle for French Legal Notices (Mentions LÃ©gales) with RGPD 2025 compliance",
    "keywords": ["Mentions Legales", "Disclaimer", "Legal Notice", "RGPD", "Symfony", "Bundle"],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Kadur Arnaud",
            "email": "contact@kadur-arnaud.fr",
            "homepage": "http://kadur-arnaud.fr"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "symfony/framework-bundle": "^5.4 || ^6.4 || ^7.0",
        "symfony/twig-bundle": "^5.4 || ^6.4 || ^7.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "rector/rector": "^1.0",
        "symfony/phpunit-bridge": "^5.4 || ^6.4 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Rudak\\DisclaimerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rudak\\DisclaimerBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}